@model HandleErrorInfo
@{
    ViewBag.Title = "Error";
}

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <h3 class="text-center">Error</h3>
        <dl class="dl-horizontal">
            <dt>Tipo</dt>
            <dd>@Model.Exception.GetType().ToString()</dd>

            <dt>Mensaje</dt>
            <dd>@Model.Exception.Message</dd>

        </dl>
    </div>
</div>
